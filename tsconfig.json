{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "bundler",
		"allowJs": true,
		"alwaysStrict": true,
		"checkJs": false,
		"declaration": false,
		"esModuleInterop": true,
		"isolatedModules": true,
		"incremental": true,
		"noEmit": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": false,
		"strict": true,
	}
}