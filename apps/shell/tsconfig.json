{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"jsx": "preserve",
		"noEmit": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@*": [
				"./src/*"
			],
			"@pages/*": [
				"./pages/*"
			]
		}
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts"
	]
}