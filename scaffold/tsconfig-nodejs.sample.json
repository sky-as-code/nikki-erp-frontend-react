{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"outDir": "dist",
		"baseUrl": "src",
		"rootDir": "src",
		"target": "es2024",
		"alwaysStrict": true,
		"declaration": false,
		"esModuleInterop": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"noEmit": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"skipLibCheck": true,
		"sourceMap": false,
		"strict": true,
	}
}